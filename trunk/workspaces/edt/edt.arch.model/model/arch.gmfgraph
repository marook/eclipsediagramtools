<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition" name="arch">
  <figures
      name="Default">
    <descriptors
        name="NetworkFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle">
        <layout
            xsi:type="gmfgraph:FlowLayout"/>
        <insets
            top="5"
            left="5"
            bottom="5"
            right="5"/>
        <children
            xsi:type="gmfgraph:Label"
            name="NetworkNameFigure"
            text="&lt;...>"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="ServerClusterFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="ServerClusterFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"/>
        <insets
            top="5"
            left="5"
            bottom="5"
            right="5"/>
        <children
            xsi:type="gmfgraph:Label"
            name="ServerClusterNameFigure"
            text="&lt;...>"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.1/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="ServerHostFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="ServerHostFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"/>
        <insets
            top="5"
            left="5"
            bottom="5"
            right="5"/>
        <children
            xsi:type="gmfgraph:Label"
            name="ServerHostNameFigure"
            text="&lt;...>"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.2/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="JavaVirtualMachineFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="JavaVirtualMachineFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"/>
        <insets
            top="5"
            left="5"
            bottom="5"
            right="5"/>
        <children
            xsi:type="gmfgraph:Label"
            name="JavaVirtualMachineNameFigure"
            text="&lt;...>"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.3/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="SoftwareComponentFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="SoftwareComponentFigure">
        <layout
            xsi:type="gmfgraph:FlowLayout"/>
        <insets
            top="5"
            left="5"
            bottom="5"
            right="5"/>
        <children
            xsi:type="gmfgraph:Label"
            name="SoftwareComponentNameFigure"
            text="&lt;...>"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.4/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="DependencyFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="DependencyFigure"
          lineWidth="2"
          lineKind="LINE_DASH"
          targetDecoration="//@figures.0/@descriptors.5/@actualFigure/@children.1">
        <children
            xsi:type="gmfgraph:Label"
            name="DependencyNameFigure"
            text="&lt;...>"/>
        <children
            xsi:type="gmfgraph:PolylineDecoration"
            name="DependencyArrowFigure"
            lineWidth="2">
          <template
              x="-1"
              y="2"/>
          <template/>
          <template
              x="-1"
              y="-2"/>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.5/@actualFigure/@children.0"/>
    </descriptors>
  </figures>
  <nodes
      name="Network"
      figure="NetworkFigure"/>
  <nodes
      name="ServerCluster"
      figure="ServerClusterFigure"/>
  <nodes
      name="ServerHost"
      figure="ServerHostFigure"/>
  <nodes
      name="JavaVirtualMachine"
      figure="JavaVirtualMachineFigure"/>
  <nodes
      name="SoftwareComponent"
      figure="SoftwareComponentFigure"/>
  <connections
      name="Dependency"
      figure="DependencyFigure"/>
  <labels
      name="NetworkName"
      figure="NetworkFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.0"/>
  <labels
      name="ServerClusterName"
      figure="ServerClusterFigure"
      accessor="//@figures.0/@descriptors.1/@accessors.0"/>
  <labels
      name="ServerHostName"
      figure="ServerHostFigure"
      accessor="//@figures.0/@descriptors.2/@accessors.0"/>
  <labels
      name="JavaVirtualMachineName"
      figure="JavaVirtualMachineFigure"
      accessor="//@figures.0/@descriptors.3/@accessors.0"/>
  <labels
      name="SoftwareComponentName"
      figure="SoftwareComponentFigure"
      accessor="//@figures.0/@descriptors.4/@accessors.0"/>
  <labels
      name="DependencyName"
      figure="DependencyFigure"
      accessor="//@figures.0/@descriptors.5/@accessors.0"/>
</gmfgraph:Canvas>
